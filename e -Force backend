[32m+[m[32m            if (typeof WebSocketVideoCallManager !== 'undefined') {[m
[32m+[m[32m                videoCallManager = new WebSocketVideoCallManager();[m
[32m+[m[32m                log(`‚úÖ Test setup complete for User: ${userId}, Partner: ${partnerId}`, 'success');[m
[32m+[m[32m            } else {[m
[32m+[m[32m                log('‚ùå WebSocketVideoCallManager not loaded', 'error');[m
[32m+[m[32m            }[m
