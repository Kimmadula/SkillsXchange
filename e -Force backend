[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            // Set global variables for the video call manager[m
[32m+[m[32m            window.userId = userId;[m
[32m+[m[32m            window.tradeId = tradeId;[m
[32m+[m[32m            window.partnerId = partnerId;[m
[32m+[m[41m            [m
[32m+[m[32m            // Show video container[m
